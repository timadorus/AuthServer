<ivysettings>
    <properties file="${ivy.settings.dir}/ivysettings-file.properties" />
    <property name="org.repo.url" value="http://timadorus-hudson.informatik.haw-hamburg.de:8081/artifactory/remote-repos" />
	<settings defaultResolver="timadorus-org-rep" />
	<caches defaultCacheDir="${ivy.cache.dir}" />
    <resolvers>
		<url name="timadorus-proxy-rep">
			<ivy pattern="${org.repo.url}/[module]/[revision]/ivy-[revision].xml" />
			<artifact pattern="${org.repo.url}/[module]/[revision]/[artifact]-[revision].[ext]" />
		</url>
    	<ibiblio name="ibiblio" />
